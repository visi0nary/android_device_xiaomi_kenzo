# IR Blaster
/dev/lirc[0-9]*                                                       u:object_r:lirc_device:s0

# Fingerprint
/dev/socket/fpce                                                      u:object_r:fpc_socket:s0
/data/goodix(/.*)?                                                    u:object_r:fpc_data_file:s0
/sys/devices/soc.0/goodix_fingerprint.*/hw_reset                      u:object_r:fpc_sysfs:s0
/sys/devices/soc.0/goodix_fingerprint.*/irq                           u:object_r:fpc_sysfs:s0
/sys/devices/soc.0/goodix_fingerprint.*/wakeup_enable                 u:object_r:fpc_sysfs:s0
